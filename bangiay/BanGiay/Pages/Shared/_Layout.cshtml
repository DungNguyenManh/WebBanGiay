<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BanGiay</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/BanBanGiay.styles.css" asp-append-version="true" />
    <link href="~/client/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/client/css/elegant-icons.css" rel="stylesheet" />
    <link href="~/client/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/client/css/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/client/css/nice-select.css" rel="stylesheet" />
    <link href="~/client/css/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/client/css/slicknav.min.css" rel="stylesheet" />
    <link href="~/client/css/style.css" rel="stylesheet" />
</head>
<body>
    @Html.Partial("_header");

    @RenderBody()

    @Html.Partial("_footer");

    <script src="~/client/js/bootstrap.min.js"></script>
    <script src="~/client/js/jquery-3.3.1.min.js"></script>
    <script src="~/client/js/jquery-ui.min.js"></script>
    <script src="~/client/js/jquery.nice-select.min.js"></script>
    <script src="~/client/js/jquery.slicknav.js"></script>
    <script src="~/client/js/main.js"></script>
    <script src="~/client/js/mixitup.min.js"></script>
    <script src="~/client/js/owl.carousel.min.js"></script>
</body>
</html>